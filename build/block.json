		"numberOfPosts": {
			"type": "number",
			"default": 4
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "bbuilds-nyt-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
