{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/bbuilds-nyt-block",
	"version": "0.1.0",
	"title": "BBuilds NYT Books API Demo Block",
	"category": "widgets",
	"icon": "hammer",
	"description": "Example block leveraging NYT Books API.",
	"attributes": {
		"apiData": {
			"type": "array",
			"default": []
		},
		"isLoading": {
			"type": "boolean",
			"default": true
		},
		"resultsList": {
			"type": "array",
			"default": []
